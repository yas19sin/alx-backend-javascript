{
    "env": {
        "browser": true,
        "es2021": true,
        "mocha": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:mocha/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "mocha"
    ],
    "rules": {
        "mocha/no-skipped-tests": "warn",
        "mocha/no-exclusive-tests": "error"
    }
}